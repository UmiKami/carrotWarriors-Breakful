{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background":{
    "service_worker":"background.js"
  },
  "permissions": [
    "scripting",
    "storage",
    "alarms"
  ],
  "host_permissions": ["<all_urls>"]
}